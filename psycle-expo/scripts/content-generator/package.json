        "bundle": "ts-node src/bundler.ts"
    },
    "dependencies": {
        "@google/generative-ai": "^0.21.0",
        "dotenv": "^16.4.5",
        "rss-parser": "^3.13.0",
        "zod": "^3.23.8"
