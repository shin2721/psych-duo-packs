{
    "name": "psycle-content-generator",
    "version": "1.0.0",
    "description": "AI-powered content generation pipeline for Psycle lessons",
    "main": "src/pipeline.ts",
    "scripts": {
        "generate": "ts-node src/pipeline.ts",
        "generate:seed": "ts-node src/generator.ts",
        "validate": "ts-node src/critic.ts",
        "watch": "ts-node src/watcher.ts",
        "watch:ai": "ts-node src/watcher.ts --ai",
